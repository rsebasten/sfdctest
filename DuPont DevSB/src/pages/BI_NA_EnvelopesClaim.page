padding-top:0px !important;
}
     .Global {
           background-color: #fff; color: #6e6e6e; font-family: Arial, Segoe UI, Sans-Serif; font-size: 12px; font-weight:normal; line-height:18px; margin: 0px; padding: 0px; text-rendering: optimizeLegibility; -webkit-font-smoothing:antialiased; 
     }
 
               .customPopup{
                    background-color: white;
                    border-style: solid;
                    color: grey;
                    font: normal normal bold 11px Trebuchet MS, Helvetica;
                    border-width: 3px;
                    border-color:grey;
