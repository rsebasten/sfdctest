<apex:page showChat="false" showHeader="true" standardController="Case" extensions="ctrlBINA_EnvelopesCreateClaim" action="{!init}">
<apex:includeScript value="{!$Resource.JQuery_to_Remove_None}"/>
<script type="text/javascript">
    $(document).ready(function() {
        $('[id*=var] option[value=""]').remove();
    }); 
</script>

<style type="text/css">
.bPageBlock .detailList tr td, .bPageBlock .detailList tr th, .hoverDetail .bPageBlock .detailList tr td, .hoverDetail .bPageBlock .detailList tr th
body .bEditBlock .pbBottomButtons, body .apexp .bPageBlock.apexDefaultPageBlock .pbBottomButtons {
 border: 0px !important; 
 color: #000 !important;
}
.labelCol{
font-size:98% !important;
font-weight:bold !important;
color: #000 !important;
font-family: Arial,Helvetica,sans-serif;
padding-left:0px !important;
padding-right:0px !important;
}
.span{
color:red;
font-size:x-large;
vertical-align:top;
