<apex:sectionHeader title="Corian® Solid Surface and Corian® Quartz Authorized Retailer"/>
<apex:pageBlock id="pgblock">
<apex:pageBlockSection id="pgblocksec">
<apex:composition template="templateDupont" >
<apex:define name="body">
<c:SiteScript script="{!script}" /> 
<!-- Added Product Selection to the page -->

<apex:outputPanel id="KeyProduct" >

<div style="margin-left:300px;" >
<c:BI_NA_MultiSelectMaterials leftLabel="Product: Available" 
                leftOption="{!AllProducts}"
                rightLabel="Product: Selected*"
                rightOption="{!SelectedProducts}"
                size="8"
                width="300px"/>  </div>
</apex:outputPanel>
<br/><br/>
<c:SiteScript script="{!script2}" />
<br/><br/>
<apex:outputPanel id="Product">
<div style="margin-left:350px;">
<apex:pageBlockSection id="Product1" columns="1">
<!---- <apex:outputLabel value="Search Product" styleClass="labelCol" style="padding-LEFT:11em;"/> --->
<apex:outputText label="Product">
<apex:inputText value="{!searchProduct}" onkeydown="if(event.keyCode==13){this.blur();actionFunction();}" id="inpTxt" style="margin-left:14px;"/>
<apex:commandButton value="Search" action="{!searchProductValue}" status="cStatus" id="searchButton" reRender="Product"/ >
</apex:outputText>
<apex:actionStatus id="cStatus" startText=" Please Wait..."  stopText="" startStyle="padding-LEFT:18em;font-weight:bold;"/>
<apex:outputText id="NoProduct" rendered="{!noProduct}" styleClass="labelCol" value=" No Product Found... Please search other product" style="margin-LEFT:150px; color:Brown !important; font-weight:bold!important;"></apex:outputText>
</apex:pageBlockSection>  </div>
<div style="margin-left:300px;">
<c:BI_NA_MultiSelectMaterials leftLabel="Showroom Display - Color: Available"
                leftOption="{!AllMaterials}"
                rightLabel="Showroom Display - Color: Selected"
                rightOption="{!SelectedMaterials}"
                size="8"
                width="300px"/>  </div>              
</apex:outputPanel>
    <br/><br/>
    <div style="margin-left:300px;">
        <apex:commandbutton id="submit" action="{!save}" value="{!$Label.site.submit}"  />
    </div>
    </apex:define>    
    </apex:composition>
    </apex:pageBlockSection>
    
