<apex:page controller="ctrlBIRetailerAuth" action="{!init}"  showHeader="false">
<apex:pagemessages />    
    <style type="text/css">
        .link{
          text-decoration:none;
          font-weight:bold;
        }
    </style>
    
    <p style="background-color:white;height:100px;width:1050px;margin-left:5px;">
