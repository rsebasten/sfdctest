<!--<Nishant 20151012>Adding Local Version Functionality for Extend Existing as part of R02 Enhancement-->
function callActionMethod(){

var txtVal1 = document.getElementById("{!$Component.MainPage.pb.PbOutExisting.Region}").value;
var txtVal3 = document.getElementById("{!$Component.MainPage.pb.PbOutExisting.LocalVersion}").value;
var txtVal4 = document.getElementById("{!$Component.MainPage.pb.PbOutExisting.exstate}").value;

/*Below Method is generated by "apex:actionFunction" which will call Apex Method "echoVal" */
RenderLocalLanguage(txtVal1,txtVal3,txtVal4);

}

var previousOnload = window.onload; 
window.onload = function() { 

    if (previousOnload) { 
    previousOnload(); 
    } 
    callActionMethod();    
    sellingCompanyDependents(); 
    SubBusiness();
} 

</script>

