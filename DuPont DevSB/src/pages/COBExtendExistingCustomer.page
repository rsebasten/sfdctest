LocalRegion(txtVal7 );
}

function checkPODRender(){
var txtVal8 = document.getElementById("{!$Component.MainPage.pb.Pb12.POD}").value;
PODRender(txtVal8);
}

function checkPODRender1(){
var txtVal9 = document.getElementById("{!$Component.MainPage.pb.Pb13.POD1}").value;
