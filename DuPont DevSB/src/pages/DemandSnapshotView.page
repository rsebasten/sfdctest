<apex:page standardController="Demand__c" extensions="DemandSnapshotManual" action="{!takeSnapshot}">
    <apex:pageBlock >
        <apex:pageBlockSection >
            <apex:outputText >
                Snapshot Taken
                <apex:form >
                    <apex:commandButton action="{!cancel}" value="Back" />
                </apex:form>
            </apex:outputText>
        </apex:pageBlockSection>
    </apex:pageBlock>
    
</apex:page>