function uploadFile(name){
if(name == "CommunicationSection"){
j$("[id$='saveCommunication']").css("display", "inline");
j$("[id$='cancelCommunication']").css("display", "inline");
}
else if(name == "BudgetSection" ){
j$("[id$='saveBudget']").css("display", "inline");
j$("[id$='cancelBudget']").css("display", "inline");
}
else if(name == "InvestigationEvidenceSection" ){
j$("[id$='saveInvestigationEvidence']").css("display", "inline");
j$("[id$='cancelInvestigationEvidence']").css("display", "inline");
}
else if(name == "RaidActionSection" ){
j$("[id$='saveRaidAction']").css("display", "inline");
j$("[id$='cancelRaidAction']").css("display", "inline");
}
else if(name == "CustomsSeizureSection" ){
j$("[id$='saveCustomsSeizure']").css("display", "inline");
j$("[id$='cancelCustomsSeizure']").css("display", "inline");
}
else if(name == "LegalSection" ){
j$("[id$='saveLegal']").css("display", "inline");
j$("[id$='cancelLegal']").css("display", "inline");
}
else if(name == "GeneralSection" ){
j$("[id$='saveCaseDetail']").css("display", "inline");
j$("[id$='cancelCaseDetail']").css("display", "inline");
}
else if(name == "ResolutionSection" ){
j$("[id$='saveResolution']").css("display", "inline");
j$("[id$='cancelResolution']").css("display", "inline");
}
else if(name == "TargetSection" ){
j$("[id$='saveTarget']").css("display", "inline");
j$("[id$='cancelTarget']").css("display", "inline");
}
}



function validateCheckboxes(){
if (j$("[id$='ProofOfUse']").prop('checked') || j$("[id$='BPCCD1']").prop('checked') || j$("[id$='BPCCD2']").prop('checked') ||
j$("[id$='BPCCDHardcopy']").prop('checked') || j$("[id$='LegalCDsent']").prop('checked') || j$("[id$='ProperUse']").prop('checked') ||
j$("[id$='ResponseFromInfringer']").prop('checked') || j$("[id$='IndustryCommunication']").prop('checked')){
j$("[id$='communications']").attr('checked','checked');
}
if (j$("[id$='DomainPriorityToolScore']").prop('checked') || j$("[id$='ThirdPartyWebCrawlers']").prop('checked') || j$("[id$='Websitescreenshot']").prop('checked') ){
j$("[id$='internetEvidence']").attr('checked','checked');
}
if (j$("[id$='CustomsSeizurebox']").prop('checked') || j$("[id$='PrivateInvestigationinitiated']").prop('checked') 
|| j$("[id$='InvestigationReportReceived']").prop('checked') || j$("[id$='Physicalvisittotarget']").prop('checked') 
|| j$("[id$='CriminalInvestigation']").prop('checked')){
j$("[id$='InvestigationAndEvidence']").attr('checked','checked');
}
if (j$("[id$='RaidConducted']").prop('checked') || j$("[id$='RaidSeizure']").prop('checked')){
j$("[id$='raidActions']").attr('checked','checked');
}
if (j$("[id$='SampleReceived']").prop('checked') || j$("[id$='SampleAnalyzed']").prop('checked') 
|| j$("[id$='TrapPurchaseAnalyzed']").prop('checked') || j$("[id$='TrapPurchase']").prop('checked') ){
j$("[id$='SamplesandTrapPurchases']").attr('checked','checked');
}
if (j$("[id$='WebsiteImage']").prop('checked') || j$("[id$='WebsiteCorrected']").prop('checked') 
|| j$("[id$='Websitetakendown']").prop('checked') || j$("[id$='SourceRevealed']").prop('checked') 
|| j$("[id$='FineImposed']").prop('checked') | j$("[id$='DomainName']").prop('checked')
|| j$("[id$='SettlementAgreement']").prop('checked') | j$("[id$='UndertakingReceived']").prop('checked')
|| j$("[id$='PublicApology']").prop('checked') | j$("[id$='CourtRuling']").prop('checked')
|| j$("[id$='Damages']").prop('checked') | j$("[id$='Arrests']").prop('checked')
|| j$("[id$='ManufacturingEquipment']").prop('checked') | j$("[id$='InfringingProducts']").prop('checked')){
j$("[id$='ResolutionRecovery']").attr('checked','checked');
}
if (j$("[id$='hardBenefit']").prop('checked') || j$("[id$='softBenefit']").prop('checked') 
|| j$("[id$='BPSoftBenefit']").prop('checked') || j$("[id$='legalRecovery']").prop('checked') 
|| j$("[id$='licensePotential']").prop('checked') ){
j$("[id$='FinancialBenefits']").attr('checked','checked');
}
if (j$("[id$='TRO']").prop('checked') ||j$("[id$='AdministrativeAction']").prop('checked') 
|| j$("[id$='CivilProceeding']").prop('checked') | j$("[id$='CriminalProceeding']").prop('checked')
|| j$("[id$='DiscoveryPeriod']").prop('checked') | j$("[id$='TrialInitiated']").prop('checked')){
j$("[id$='LegalAction']").attr('checked','checked');
}
}

