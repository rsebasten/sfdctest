<apex:page standardController="DuPontM__Category__c" extensions="DuPontM.ContentDisplayController" sidebar="false" showHeader="false">
    <apex:pageBlock >
    <apex:pageBlockTable value="{!content}" var="con">
        <apex:column headerValue="Title">
            <apex:outputlink value="/{!con.Id}" target="_blank">{!con.Title}</apex:outputlink>
        </apex:column>
        <apex:column headerValue="Owner">
            {!con.Owner.Name}
        </apex:column>
        <apex:column headerValue="File Type">
            {!con.FileType}
        </apex:column>
        <apex:column headerValue="Document Type">
            {!con.DuPontM__Document_Type__c}
        </apex:column>
        
        
    </apex:pageBlockTable>
    </apex:pageBlock>
</apex:page>