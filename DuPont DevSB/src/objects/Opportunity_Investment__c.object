    <fields>
        <fullName>fDistributor_Total_Investment__c</fullName>
        <description>Formula field that calculates the various the overall Distributor Investment on the Opportunity Investment options - some are time &amp; term based, some are flat, some are % percent based.</description>
        <externalId>false</externalId>
        <formula>CASE(Type__c,
&quot;One-Time Allowance&quot;, Distributor_Payment__c,
&quot;Products&quot; , Distributor_Payment__c,
&quot;Paint &amp; Equipment&quot; , Distributor_Payment__c,
&quot;Rebate&quot;, (Term__c/12)*(Distributor_Percentage__c)* Opportunity__r.Amount,
&quot;Recurring Allowance&quot;,Distributor_Payment__c*fFrequencyTerm__c,0
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Distributor Total Investment</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>fDuPont_Total_Investment__c</fullName>
        <description>Formula field that calculates the DuPont Total Investment for various Opportunity Investment options - some are time &amp; term based, some are flat, some are % percent based.</description>
        <externalId>false</externalId>
        <formula>CASE(Type__c, 
&quot;One-Time Allowance&quot;, DuPont_Payment__c, 
&quot;Products&quot; , DuPont_Payment__c, 
&quot;Paint &amp; Equipment&quot; , DuPont_Payment__c, 
&quot;Rebate&quot;, (Term__c/12)*(DuPont_Percentage__c)* Opportunity_Amount__c, 
&quot;Prebate&quot;, DuPont_Payment__c,
&quot;Recurring Allowance&quot;,DuPont_Payment__c*fFrequencyTerm__c,0 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>DuPont Total Investment</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>fFrequencyTerm__c</fullName>
        <description>Determines the number of terms based on the frequency unit.</description>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Frequency__c ,&quot;Monthly&quot;), Term__c,
IF(ISPICKVAL(Frequency__c ,&quot;Quarterly&quot;), Term__c/3,
IF(ISPICKVAL(Frequency__c ,&quot;Annual&quot;), Term__c/12, 0)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Frequency Term</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
