    <validationRules>
        <fullName>Opp_Product_Line_Description_is_Require</fullName>
        <active>true</active>
        <description>Requires a value in the Line description field before a Opportunity Product record can be saved for the specified opportunity record type(s).</description>
        <errorConditionFormula>AND ( 
Opportunity.RecordType.DeveloperName = &quot;DEC_PVAM_Opportunity&quot;, 
ISBLANK( Description ) 
)</errorConditionFormula>
        <errorDisplayField>Description</errorDisplayField>
        <errorMessage>Please provide the product code in the Line Description field.</errorMessage>
    </validationRules>
