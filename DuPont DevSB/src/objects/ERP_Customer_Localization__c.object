    <fields>
        <fullName>fLocal_Language__c</fullName>
        <description>Displays the Local Language description for the Nation Key value in ERP external ID</description>
        <externalId>false</externalId>
        <formula>IF(TRIM(RIGHT(External_ERP_ID__c, 2)) = &quot;-1&quot;, &quot;Thai&quot;, 
IF(TRIM(RIGHT(External_ERP_ID__c, 2)) = &quot;-8&quot; , &quot;Ukrainian_Cyril&quot;, 
IF(TRIM(RIGHT(External_ERP_ID__c, 2)) = &quot;-A&quot; , &quot;Arabic&quot;, 
IF(TRIM(RIGHT(External_ERP_ID__c, 2)) = &quot;-B&quot; , &quot;Hebrew&quot;, 
IF(TRIM(RIGHT(External_ERP_ID__c, 2)) = &quot;-C&quot; , &quot;Chinese&quot;, 
IF(TRIM(RIGHT(External_ERP_ID__c, 2)) = &quot;-G&quot; , &quot;Greek&quot;, 
IF(TRIM(RIGHT(External_ERP_ID__c, 2)) = &quot;-H&quot;, &quot;Hangul&quot;, 
IF(TRIM(RIGHT(External_ERP_ID__c, 2)) = &quot;-K&quot; , &quot;Kanji&quot;, 
IF(TRIM(RIGHT(External_ERP_ID__c, 2)) = &quot;-M&quot;, &quot;Chinese trad.&quot;, 
IF(TRIM(RIGHT(External_ERP_ID__c, 2)) = &quot;-N&quot;, &quot;Katakana&quot;, 
IF(TRIM(RIGHT(External_ERP_ID__c, 2)) = &quot;-R&quot; , &quot;Cyrillic&quot;, &quot;English&quot;)))))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Local Language</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
