    <validationRules>
        <fullName>UserOrContact</fullName>
        <active>true</active>
        <description>Rule that prevents a  user from entering a user and a contact into the call report contact object.</description>
        <errorConditionFormula>AND(
   NOT(ISBLANK(User__c)),
   NOT(ISBLANK(Contact__c))
)</errorConditionFormula>
        <errorMessage>You can select either a contact or a user, not both.</errorMessage>
    </validationRules>
