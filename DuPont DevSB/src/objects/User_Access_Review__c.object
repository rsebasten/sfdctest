    <webLinks>
        <fullName>Review</fullName>
        <availability>online</availability>
        <description>Button to trigger the custom visualforce page to view collated User Access Review Details Data in single page</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Review</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/36.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/36.0/apex.js&quot;)} 

if(!{!ISBLANK(User_Access_Review__c.Snapshot_Taken_By__c)}){ 

window.location = &apos;/apex/AUAReviewPage?scontrolCaching=1&amp;id={!User_Access_Review__c.Id}&apos;;

}else{
alert(&quot;The snapshot has not been taken for this Audit Process. Please contact support or wait for its completion.&quot;);
}</url>
    </webLinks>
    <webLinks>
        <fullName>Take_Snapshot</fullName>
        <availability>online</availability>
        <description>Execute batch to capture User Information along with security data in the object</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Take Snapshot</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/36.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/36.0/apex.js&quot;)}

var cnfrm = false;

if(!{!ISBLANK(User_Access_Review__c.Snapshot_Taken_By__c)}){
  cnfrm = false;
  alert(&quot;The snapshot has already been taken for this Audit Process.&quot;);
}else{
   cnfrm   = confirm(&quot;Are you sure to Take Snapshot. This process cannot be reverted?&quot;);
}

if(cnfrm == true)
{
    sforce.apex.execute(&quot;AUATakeSnapshot&quot;,&quot;startSnapshotProcess&quot;,{AuditProcessId:&quot;{!User_Access_Review__c.Id}&quot;});
    alert(&quot;Snapshot will now be taken asynchronously in the background. The Snapshot Status field will be changed to \&quot;Completed\&quot;, once the Snapshot is taken successfully.&quot;);
    window.location.reload();
}</url>
    </webLinks>
