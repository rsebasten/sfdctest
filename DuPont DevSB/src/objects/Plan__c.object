    <fields>
        <fullName>fPlan_Hierarchy__c</fullName>
        <description>Displays the ancestors of this plan. (max 5 levels)</description>
        <externalId>false</externalId>
        <formula>IF( Parent_Plan__c==&quot;&quot;,
	&quot;&gt;&gt;&quot;+ Name+&quot;&gt;&quot;, 
    IF(Parent_Plan__r.Parent_Plan__c==&quot;&quot;,
    &quot;&gt;&gt;&quot;+Parent_Plan__r.Name+
    &quot;&gt;&quot;+ Name+&quot;&gt;&quot;,
         IF(Parent_Plan__r.Parent_Plan__r.Parent_Plan__c==&quot;&quot;,
         &quot;&gt;&gt;&quot;+Parent_Plan__r.Parent_Plan__r.Name+
         &quot;&gt;&quot;+Parent_Plan__r.Name+
         &quot;&gt;&quot;+ Name+&quot;&gt;&quot;,
         	IF( Parent_Plan__r.Parent_Plan__r.Parent_Plan__r.Parent_Plan__c==&quot;&quot;,
         	&quot;&gt;&gt;&quot;+Parent_Plan__r.Parent_Plan__r.Parent_Plan__r.Name+
                &quot;&gt;&quot;+Parent_Plan__r.Parent_Plan__r.Name+
                &quot;&gt;&quot;+Parent_Plan__r.Name+
                &quot;&gt;&quot;+ Name+&quot;&gt;&quot;,
                       IF( Parent_Plan__r.Parent_Plan__r.Parent_Plan__r.Parent_Plan__r.Parent_Plan__c==&quot;&quot;,
                        &quot;&gt;&gt;&quot;+Parent_Plan__r.Parent_Plan__r.Parent_Plan__r.Parent_Plan__r.Name+
         		&quot;&gt;&quot;+Parent_Plan__r.Parent_Plan__r.Parent_Plan__r.Name+
                        &quot;&gt;&quot;+Parent_Plan__r.Parent_Plan__r.Name+
                        &quot;&gt;&quot;+Parent_Plan__r.Name+
                        &quot;&gt;&quot;+ Name+&quot;&gt;&quot;,
                              &quot;&gt;Error: Too many ancestors&gt;&quot;
                              )
	)
              )
    )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Displays the ancestors of this plan. (max 5 levels)</inlineHelpText>
