    <fields>
        <fullName>fHierarchy__c</fullName>
        <externalId>false</externalId>
        <formula>IF(fLevel__c==0,&apos;root&apos;, 
IF(fLevel__c==1,Name,
IF(fLevel__c==2,Parent__r.Name+&quot;&gt;&quot;+ Name,
IF(fLevel__c==3,Parent__r.Parent__r.Name+&quot;&gt;&quot;+Parent__r.Name+&quot;&gt;&quot;+ Name,
IF(fLevel__c==4,Parent__r.Parent__r.Parent__r.Name+&quot;&gt;&quot;+Parent__r.Parent__r.Name+&quot;&gt;&quot;+Parent__r.Name+&quot;&gt;&quot;+ Name,
&quot;&gt;Error: Too many ancestors&gt;&quot;
)))))</formula>
        <label>Hierarchy (fx)</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>fLevel__c</fullName>
        <description>Max out at Level 3.</description>
        <externalId>false</externalId>
        <formula>IF( Parent__c==&quot;&quot;,0,
IF(Parent__r.Parent__c==&quot;&quot;,1,
IF(Parent__r.Parent__r.Parent__c==&quot;&quot;,2,
IF(Parent__r.Parent__r.Parent__r.Parent__c==&quot;&quot;,3
,4
))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Level (fx)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
