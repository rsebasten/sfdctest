    </fields>
    <fields>
        <fullName>fReasonName__c</fullName>
        <description>Reason Type - Root Reason</description>
        <externalId>false</externalId>
        <formula>CASE(fLevel__c,0,Name,
1,Parent__r.Name,
2,Parent__r.Parent__r.Name,
3,Parent__r.Parent__r.Parent__r.Name,
&quot;&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Reason Name (fx)</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
