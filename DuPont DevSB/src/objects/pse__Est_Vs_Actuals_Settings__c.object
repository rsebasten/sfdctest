<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>Configuration for Estimate Vs Actuals</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>pse__Adjust_Past_Est_on_Records_with_Actuals__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Whether the Est Vs Actuals fields derived from the schedule (e.g., Estimated Hours) should be updated for Est Vs Actuals records that have an end date prior to the start of this week.</inlineHelpText>
        <label>Adjust Past Est on Records with Actuals</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>pse__Delete_Out_of_Range_Records_With_Actuals__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Delete Est vs Actuals records even if they have actuals set for records that are no longer within a modified schedule&apos;s date range. NOTE: Records without actuals will always be deleted.</description>
        <externalId>false</externalId>
        <inlineHelpText>Delete Est vs Actuals records even if they have actuals set for records that are no longer within a modified schedule&apos;s date range. NOTE: Records without actuals will always be deleted.</inlineHelpText>
        <label>Delete Out of Range Records With Actuals</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>pse__Generate_Monthly_Records__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Whether to generate Est Vs Actuals records by month</inlineHelpText>
        <label>Generate Monthly Records</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>pse__Generate_Quarterly_Records__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Whether to generate Est Vs Actuals records by quarter</inlineHelpText>
        <label>Generate Quarterly Records</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>pse__Generate_Weekly_Records__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Whether to generate est vs actuals by week</inlineHelpText>
        <label>Generate Weekly Records</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>pse__Process_Out_Of_Range_Timecards__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Create Est Vs Actuals records for timecards that have hours outside of the assignment&apos;s start and end date. The Est Vs Actuals records will have zero values for estimates on these records.</description>
        <externalId>false</externalId>
        <inlineHelpText>Create Est Vs Actuals records for timecards that have hours outside of the assignment&apos;s start and end date. The Est Vs Actuals records will have zero values for estimates on these records.</inlineHelpText>
        <label>Process Out Of Range Timecards</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
