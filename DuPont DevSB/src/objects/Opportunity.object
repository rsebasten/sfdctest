    <validationRules>
        <fullName>Stage_Should_be_End_Won</fullName>
        <active>true</active>
        <description>For Status = Won, the Stage should be End - Won</description>
        <errorConditionFormula>AND( ISPICKVAL(Status__c, &quot;Won&quot;) , NOT( ISPICKVAL(StageName , &quot;End - Won&quot;) ) )</errorConditionFormula>
        <errorMessage>For an opportunity that is in Status = Won, you must change the Stage to End-Won.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Stage_Should_be_Lost</fullName>
        <active>true</active>
        <description>For Status = Lost, the Stage should be End - Lost</description>
        <errorConditionFormula>AND( ISPICKVAL(Status__c, &quot;Lost&quot;) , NOT( ISPICKVAL(StageName , &quot;End - Lost&quot;) ) )</errorConditionFormula>
        <errorMessage>For an opportunity that is in Status = Lost, you must change the Stage to End-Lost.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_Validation</fullName>
        <active>false</active>
        <description>This validates that the Start Date is earlier than then Close Date</description>
        <errorConditionFormula>OR   (
                     (Start_Date__c &gt; CloseDate) ,
                     ISNULL(  Start_Date__c)
           )</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>The Start Date must be earlier than or equal to the Close Date.</errorMessage>
    </validationRules>
