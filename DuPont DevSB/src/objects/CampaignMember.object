        <fullName>fCountry__c</fullName>
        <externalId>false</externalId>
        <formula>If(Contact.Id!=null
,IF(TEXT(Contact.Contact_Country__c)!=null
     ,TEXT(Contact.Contact_Country__c)
     ,TEXT(Contact.Account.Country__c)
     )
,IF(TEXT(Lead.Lead_Country__c)!=null
     ,TEXT(Lead.Lead_Country__c)
     ,null
     )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Country</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
