    <validationRules>
        <fullName>Optin_Management</fullName>
        <active>true</active>
        <description>Rule that validates if a user has been given the Opt-in Management rights (Campaign Member), which helps ensure leads are contacts are not opted in to Campaigns without clear permission.</description>
        <errorConditionFormula>(Campaign.RecordTypeId ==&apos;01230000000QT37&apos; ) &amp;&amp;
($Profile.Name &lt;&gt;  &apos;System Administrator&apos; ) &amp;&amp;
NOT(INCLUDES($User.SpecialRights__c,&apos;Opt-In Management&apos;) ) &amp;&amp;
( CONTAINS( LOWER(TEXT(Status)), &apos;opt-in&apos;) || CONTAINS(LOWER(TEXT(Status)), &apos;opt-out&apos;) )</errorConditionFormula>
        <errorMessage>Opt-in Management permissions not available, please use another Member Status value or check with your Key User or System Administrator.</errorMessage>
    </validationRules>
